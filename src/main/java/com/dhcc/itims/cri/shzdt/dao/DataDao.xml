<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dhcc.itims.cri.shzdt">
    <resultMap type="com.dhcc.itims.cri.shzdt.dao.ProtypeData" id="ProtypeDataMap">
        <result column="id" property="id"/>
        <result column="parameterCode" property="parameterCode"/>
        <result column="parameterName" property="parameterName"/>
        <result column="value" property="value"/>
        <result column="unit" property="unit"/>
        <result column="state" property="state"/>
        <result column="alarmTop" property="alarmTop"/>
        <result column="alarmBottom" property="alarmBottom"/>
        <result column="alertTop" property="alertTop"/>
        <result column="alertBottom" property="alertBottom"/>
        <result column="collectTime" property="collectTime"/>
    </resultMap>
    <select id="selectAllData" resultMap="ProtypeDataMap">
        SELECT id,
		   parameterCode,
		   parameterName,
		   value,
		   unit,
		   state,
		   alarmTop,
		   alarmBottom,
		   alertTop,
		   alertBottom,
		   collectTime
        FROM shzdt_data
    </select>
</mapper>